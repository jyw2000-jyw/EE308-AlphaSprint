passwordinput:function(e){
    var val=e.detail.value;
    if (val!=''){
      this.setData({
        password:val
      });
      if(this.data.username !=""){
        this.setData({
          loginbtnstate:false
        })
      }
    }
      else{
        this.setData({
          loginbtnstate:true
        })
      
     }
  },  
